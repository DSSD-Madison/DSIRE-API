{
  "name": "dsire-api",
  "version": "0.0.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/DSSD-Madison/DSIRE-API.git",
    "directory": "api/"
  },
  "scripts": {
    "check": "tsc -p tsconfig.json --noEmit",
    "really-check": "tsc -p tsconfig.json --noEmit --noErrorTruncation",
    "build": "esbuild src/index.ts --bundle --external:@prisma/client --platform=node --target=node18 --sourcemap=inline --outfile=../dist/api/index.cjs --metafile=meta.json",
    "build-main": "esbuild src/index.ts --bundle --external:@prisma/client --platform=node --target=node18 --minify --outfile=../dist/api/index.cjs",
    "graphql": "ts-node src/handlers/graphql/devServer.ts",
    "clean": "rm -rf ../dist"
  },
  "dependencies": {
    "@aws-sdk/types": "^3.310.0",
    "@prisma/client": "^4.13.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.15.13",
    "@types/nodemailer": "^6.4.7",
    "esbuild": "^0.17.18",
    "graphql": "^16.6.0",
    "jose": "^4.14.1",
    "nodemailer": "^6.9.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "graphql-yoga": "^3.9.1",
    "prisma": "^4.13.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}
