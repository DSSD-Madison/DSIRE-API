{
  "name": "dsire-api",
  "version": "0.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/DSSD-Madison/DSIRE-API.git",
    "directory": "api/"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json -w",
    "preserve-graphql": "mkdir -p ../dist/api; ln -sf ../../api/node_modules ../dist/api/; cp GraphiQL.mjs ../dist/api/",
    "serve-graphql": "node ../dist/api/GraphiQL.mjs",
    "clean": "rm -rf ../dist/api"
  },
  "dependencies": {
    "@aws-sdk/types": "^3.306.0",
    "@prisma/client": "^4.12.0",
    "@types/aws-lambda": "^8.10.114",
    "@types/node": "^18.15.11",
    "@types/nodemailer": "^6.4.7",
    "graphql": "^15.3.0",
    "graphql-parse-resolve-info": "^4.13.0",
    "jose": "^4.13.1",
    "nodemailer": "^6.9.1",
    "typescript": "^5.0.4",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "prisma": "^4.12.0"
  }
}
